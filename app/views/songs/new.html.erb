<h1> Song Form </h1>
<%= form_tag songs_path do %>
    <label>Name:</label><br>
  <%= text_field_tag name= "song[name]" %><br>
    <label>Artist:</label><br>
  <%= text_field_tag artist_id= "song[artist_id]" %><br>
    <label>Genre:</label><br>
  <%= text_field_tag genre_id= "song[genre_id]" %><br>
  <%= submit_tag "Create Song" %>
<% end %>